<MudDialog OnBackdropClick="@OnBackdropClick">
    <TitleContent>
        Title: @_textTest
    </TitleContent>
</MudDialog>

@code {
    private string? _textTest;

    [CascadingParameter]
    private IMudDialogInstance MudDialog { get; set; } = null!;

    private void OnBackdropClick()
    {
        _textTest = "Backdrop clicked";
        MudDialog.StateHasChanged();
    }
}